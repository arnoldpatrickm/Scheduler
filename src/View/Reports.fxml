            <SepiaTone />
         </effect>
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </RadioButton>
      <RadioButton layoutX="549.0" layoutY="57.0" mnemonicParsing="false" text="Total Customers by ..." toggleGroup="$ReportToggleGroup">
