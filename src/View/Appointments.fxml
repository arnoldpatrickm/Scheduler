            <TableColumn prefWidth="95.0" text="Type" />
            <TableColumn prefWidth="102.0" text="Start Time" />
            <TableColumn prefWidth="94.0" text="End Time" />
            <TableColumn prefWidth="88.0" text="Start Date" />
            <TableColumn prefWidth="103.0" text="End Date" />
            <TableColumn prefWidth="104.0" text="Customer ID" />
            <TableColumn prefWidth="95.0" text="User ID" />
